         point="org.eclipse.ui.handlers">
      <handler
            class="org.jetbrains.kotlin.ui.debug.commands.KotlinStepIntoSelectionHandler"
            commandId="org.eclipse.jdt.debug.ui.commands.StepIntoSelection">
         <activeWhen>
            <with
                  variable="activeEditor">
               <instanceof
                     value="org.jetbrains.kotlin.ui.editors.KotlinEditor">
               </instanceof>
            </with>
         </activeWhen>
      </handler>
   </extension>
   <extension
         point="org.eclipse.ltk.core.refactoring.renameParticipants">
      <renameParticipant
            class="org.jetbrains.kotlin.ui.refactorings.rename.KotlinTypeRenameParticipant"
            id="org.jetbrains.kotlin.ui.refacotrings.renameType"
            name="Kotlin Type Rename Participant">
         <enablement>
            <with
                  variable="affectedNatures">
               <iterate
                     ifEmpty="false"
                     operator="or">
                  <equals
                        value="org.jetbrains.kotlin.core.kotlinNature">
                  </equals>
               </iterate>
            </with>
            <with
                  variable="element">
               <instanceof
                     value="org.eclipse.jdt.core.IType">
               </instanceof>
            </with>
         </enablement>
      </renameParticipant>
      <renameParticipant
            class="org.jetbrains.kotlin.ui.refactorings.rename.KotlinFunctionRenameParticipant"
            id="org.jetbrains.kotlin.ui.refacotrings.renameFunction"
            name="Kotlin Function Rename Participant">
         <enablement>
            <with
                  variable="affectedNatures">
               <iterate
                     ifEmpty="false"
                     operator="or">
                  <equals
                        value="org.jetbrains.kotlin.core.kotlinNature">
                  </equals>
               </iterate>
            </with>
            <with
                  variable="element">
               <instanceof
                     value="org.eclipse.jdt.core.IMethod">
               </instanceof>
            </with>
         </enablement>
      </renameParticipant>
      <renameParticipant
            class="org.jetbrains.kotlin.ui.refactorings.rename.KotlinLocalRenameParticipant"
            id="org.jetbrains.kotlin.ui.refacotrings.renameLocalElement"
            name="Kotlin Local Rename Participant">
         <enablement>
            <with
                  variable="affectedNatures">
               <iterate
                     operator="or">
                  <equals
                        value="org.jetbrains.kotlin.core.kotlinNature">
                  </equals>
               </iterate>
            </with>
            <with
                  variable="element">
               <instanceof
                     value="org.jetbrains.kotlin.psi.KtElement">
               </instanceof>
            </with>
         </enablement>
      </renameParticipant>
