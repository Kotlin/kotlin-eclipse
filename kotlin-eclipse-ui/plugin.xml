<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="org.jetbrains.kotlin.ui.editor.textHover" name="Kotlin Text Hover" schema="schema/org.jetbrains.kotlin.ui.editor.textHover.exsd"/>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="org.jetbrains.kotlin.wizards"
            name="Kotlin">
      </category>
      <wizard
            category="org.jetbrains.kotlin.wizards"
            class="org.jetbrains.kotlin.wizards.NewProjectWizard"
            finalPerspective="org.jetbrains.kotlin.perspective"
            icon="icons/newprj_kotlin_wiz.gif"
            id="org.jetbrains.kotlin.projectWizard"
            name="Kotlin Project"
            preferredPerspectives="org.jetbrains.kotlin.perspective,org.eclipse.jdt.ui.JavaPerspective"
            project="true">
         <description>
            Create a new Kotlin project
         </description>
      </wizard>
      <wizard
            category="org.jetbrains.kotlin.wizards"
            class="org.jetbrains.kotlin.wizards.NewUnitWizard"
            finalPerspective="org.jetbrains.kotlin.perspective"
            hasPages="true"
            icon="icons/newfile_wiz.gif"
            id="org.jetbrains.kotlin.wizard"
            name="Kotlin File"
            preferredPerspectives="org.jetbrains.kotlin.perspective,org.eclipse.jdt.ui.JavaPerspective"
            project="false">
           <description>
              Create a new Kotlin file
           </description>
      </wizard>
      <wizard
            category="org.jetbrains.kotlin.wizards"
            class="org.jetbrains.kotlin.wizards.NewClassWizard"
            finalPerspective="org.jetbrains.kotlin.perspective"
            hasPages="true"
            icon="icons/newclass_wiz.gif"
            id="org.jetbrains.kotlin.classWizard"
            name="Kotlin Class"
            preferredPerspectives="org.jetbrains.kotlin.perspective"
            project="false">
         <description>
            Create a new Kotlin class
         </description>
      </wizard>
      <wizard
            category="org.jetbrains.kotlin.wizards"
            class="org.jetbrains.kotlin.wizards.NewSealedClassWizard"
            finalPerspective="org.jetbrains.kotlin.perspective"
            hasPages="true"
            icon="icons/newclass_wiz.gif"
            id="org.jetbrains.kotlin.classWizard"
            name="Kotlin sealed Class"
            preferredPerspectives="org.jetbrains.kotlin.perspective"
            project="false">
         <description>
            Create a new Kotlin sealed class
         </description>
      </wizard>
      <wizard
            category="org.jetbrains.kotlin.wizards"
            class="org.jetbrains.kotlin.wizards.NewInterfaceWizard"
            finalPerspective="org.jetbrains.kotlin.perspective"
            hasPages="true"
            icon="icons/newint_wiz.gif"
            id="org.jetbrains.kotlin.traitWizard"
            name="Kotlin Interface"
            preferredPerspectives="org.jetbrains.kotlin.perspective"
            project="false">
         <description>
            Create a new Kotlin interface
         </description>
      </wizard>
      <wizard
            category="org.jetbrains.kotlin.wizards"
            class="org.jetbrains.kotlin.wizards.NewSealedInterfaceWizard"
            finalPerspective="org.jetbrains.kotlin.perspective"
            hasPages="true"
            icon="icons/newint_wiz.gif"
            id="org.jetbrains.kotlin.traitWizard"
            name="Kotlin sealed Interface"
            preferredPerspectives="org.jetbrains.kotlin.perspective"
            project="false">
         <description>
            Create a new Kotlin sealed interface
         </description>
      </wizard>
      <wizard
            category="org.jetbrains.kotlin.wizards"
            class="org.jetbrains.kotlin.wizards.NewObjectWizard"
            finalPerspective="org.jetbrains.kotlin.perspective"
            hasPages="true"
            icon="icons/newobj_wiz.gif"
            id="org.jetbrains.kotlin.objectWizard"
            name="Kotlin Object"
            preferredPerspectives="org.jetbrains.kotlin.perspective"
            project="false">
         <description>
            Create a new Kotlin object
         </description>
      </wizard>
      <wizard
            category="org.jetbrains.kotlin.wizards"
            class="org.jetbrains.kotlin.wizards.NewEnumWizard"
            finalPerspective="org.jetbrains.kotlin.perspective"
            hasPages="true"
            icon="icons/newenum_wiz.gif"
            id="org.jetbrains.kotlin.enumWizard"
            name="Kotlin Enum"
            preferredPerspectives="org.jetbrains.kotlin.perspective"
            project="false">
         <description>
            Create a new Kotlin enum
         </description>
      </wizard>
      <wizard
            category="org.jetbrains.kotlin.wizards"
            class="org.jetbrains.kotlin.wizards.NewDataClassWizard"
            finalPerspective="org.jetbrains.kotlin.perspective"
            hasPages="true"
            icon="icons/newclass_wiz.gif"
            id="org.jetbrains.kotlin.enumWizard"
            name="Kotlin Data Class"
            preferredPerspectives="org.jetbrains.kotlin.perspective"
            project="false">
         <description>
            Create a new Kotlin data class
         </description>
      </wizard>
      <wizard
            category="org.jetbrains.kotlin.wizards"
            class="org.jetbrains.kotlin.wizards.NewAnnotationWizard"
            finalPerspective="org.jetbrains.kotlin.perspective"
            hasPages="true"
            icon="icons/newclass_wiz.gif"
            id="org.jetbrains.kotlin.enumWizard"
            name="Kotlin Annotation Class"
            preferredPerspectives="org.jetbrains.kotlin.perspective"
            project="false">
         <description>
            Create a new Kotlin annotation
         </description>
      </wizard>
      <wizard
            category="org.jetbrains.kotlin.wizards"
            class="org.jetbrains.kotlin.wizards.NewScriptWizard"
            finalPerspective="org.jetbrains.kotlin.perspective"
            hasPages="true"
            icon="icons/newfile_wiz.gif"
            id="org.jetbrains.kotlin.scriptWizard"
            name="Kotlin Script"
            preferredPerspectives="org.jetbrains.kotlin.perspective"
            project="false">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.editorActions">
      <editorContribution
            id="org.jetbrains.kotlin.ui.debug.KotlinFileEditor.BreakpointRulerActions"
            targetID="org.jetbrains.kotlin.ui.editors.KotlinFileEditor">
         <action
               actionID="RulerDoubleClick"
               class="org.eclipse.debug.ui.actions.RulerToggleBreakpointActionDelegate"
               id="org.jetbrains.kotlin.ui.debug.actions.ManageBreakpointRulerAction"
               label="Toggle &amp;Breakpoint">
         </action>
      </editorContribution>
      <editorContribution
            id="org.jetbrains.kotlin.ui.debug.KotlinClassFileEditor.BreakpointRulerActions"
            targetID="org.jetbrains.kotlin.ui.editors.KotlinClassFileEditor">
         <action
               actionID="RulerDoubleClick"
               class="org.eclipse.debug.ui.actions.RulerToggleBreakpointActionDelegate"
               id="org.jetbrains.kotlin.ui.debug.actions.ManageBreakpointRulerAction"
               label="Toggle &amp;Breakpoint">
         </action>
      </editorContribution>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <newWizardShortcut
               id="org.jetbrains.kotlin.wizard">
         </newWizardShortcut>
      </perspectiveExtension>
      <perspectiveExtension
            targetID="org.jetbrains.kotlin.perspective">
         <view
               id="org.eclipse.jdt.ui.PackageExplorer"
               minimized="false"
               ratio="0.3"
               relationship="left"
               relative="org.eclipse.ui.editorss"
               visible="true">
         </view>
         <view
               id="org.eclipse.ui.views.ProblemView"
               minimized="false"
               relationship="bottom"
               relative="org.eclipse.ui.editorss"
               visible="true">
         </view>
         <view
               id="org.eclipse.ui.console.ConsoleView"
               minimized="false"
               relationship="stack"
               relative="org.eclipse.ui.views.ProblemView"
               visible="true">
         </view>
         <view
               id="org.eclipse.ui.views.ContentOutline"
               minimized="false"
               relationship="right"
               relative="org.eclipse.ui.editorss"
               visible="true">
         </view>
         <newWizardShortcut
               id="org.jetbrains.kotlin.wizard">
         </newWizardShortcut>
         <newWizardShortcut
               id="org.jetbrains.kotlin.classWizard">
         </newWizardShortcut>
         <newWizardShortcut
               id="org.jetbrains.kotlin.traitWizard">
         </newWizardShortcut>
         <newWizardShortcut
               id="org.jetbrains.kotlin.objectWizard">
         </newWizardShortcut>
         <newWizardShortcut
               id="org.jetbrains.kotlin.enumWizard">
         </newWizardShortcut>
         <newWizardShortcut
               id="org.eclipse.jdt.ui.wizards.NewPackageCreationWizard">
         </newWizardShortcut>
         <newWizardShortcut
               id="org.eclipse.jdt.ui.wizards.NewClassCreationWizard">
         </newWizardShortcut>
         <newWizardShortcut
               id="org.eclipse.jdt.ui.wizards.NewInterfaceCreationWizard">
         </newWizardShortcut>
         <newWizardShortcut
               id="org.eclipse.jdt.ui.wizards.NewSourceFolderCreationWizard">
         </newWizardShortcut>
         <newWizardShortcut
               id="org.eclipse.ui.wizards.new.folder">
         </newWizardShortcut>
         <newWizardShortcut
               id="org.eclipse.ui.wizards.new.file">
         </newWizardShortcut>
         <newWizardShortcut
               id="org.jetbrains.kotlin.projectWizard">
         </newWizardShortcut>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="org.jetbrains.kotlin.preferences.KotlinPreferencePage"
            id="kotlin-eclipse-ui.preferences"
            name="Kotlin">
      </page>
      <page
            category="kotlin-eclipse-ui.preferences"
            class="org.jetbrains.kotlin.preferences.style.WorkspaceCodeStylePropertyPage"
            id="kotlin-eclipse-ui.preferences.style"
            name="Code style">
      </page>
      <page
            category="kotlin-eclipse-ui.preferences"
            class="org.jetbrains.kotlin.preferences.compiler.WorkspaceCompilerPropertyPage"
            id="kotlin-eclipse-ui.preferences.compiler"
            name="Compiler">
      </page>
      <page
            category="kotlin-eclipse-ui.preferences"
            class="org.jetbrains.kotlin.preferences.building.WorkspaceBuildingPropertyPage"
            id="kotlin-eclipse-ui.preferences.compiler"
            name="Building">
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.jetbrains.kotlin.ui.editors.KotlinFileEditor"
            contributorClass="org.eclipse.jdt.internal.ui.javaeditor.CompilationUnitEditorActionContributor"
            extensions="kt"
            icon="icons/kotlin-file.gif"
            symbolicFontName="org.eclipse.jdt.ui.editors.textfont"
            id="org.jetbrains.kotlin.ui.editors.KotlinFileEditor"
            name="Kotlin Editor">
      </editor>
      <editor
            class="org.jetbrains.kotlin.ui.editors.KotlinScriptEditor"
            contributorClass="org.eclipse.jdt.internal.ui.javaeditor.CompilationUnitEditorActionContributor"
            default="false"
            extensions="kts"
            icon="icons/kotlin-file.gif"
            symbolicFontName="org.eclipse.jdt.ui.editors.textfont"
            id="org.jetbrains.kotlin.ui.editors.KotlinScriptEditor"
            name="Kotlin Script Editor">
      </editor>
      <editor
            class="org.jetbrains.kotlin.ui.editors.KotlinExternalReadOnlyEditor"
            contributorClass="org.eclipse.jdt.internal.ui.javaeditor.CompilationUnitEditorActionContributor"
            default="false"
            extensions="kt"
            icon="icons/kotlin-file.gif"
            symbolicFontName="org.eclipse.jdt.ui.editors.textfont"
            id="org.jetbrains.kotlin.ui.editors.KotlinExternalReadOnlyEditor"
            name="Kotlin External Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.jetbrains.kotlin.ui.editors.KotlinClassFileEditor"
            extensions="class"
            icon="icons/kotlin-file.gif"
            symbolicFontName="org.eclipse.jdt.ui.editors.textfont"
            id="org.jetbrains.kotlin.ui.editors.KotlinClassFileEditor"
            name="Kotlin Class File Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="org.jetbrains.kotlin.ui.commands.psiVisualization.PsiVisualization"
            id="org.jetbrains.kotlin.psi.visualization"
            name="Psi Visualization">
      </command>
      <command
            defaultHandler="org.jetbrains.kotlin.ui.commands.j2k.JavaToKotlinActionHandler"
            id="org.jetbrains.kotlin.ui.commands.j2k"
            name="Convert Java to Kotlin">
      </command>
      <command
            defaultHandler="org.jetbrains.kotlin.ui.commands.DeconfigureKotlinActionHandler"
            id="org.jetbrains.kotlin.ui.commands.deconfiguration"
            name="Deconfigure Kotlin">
      </command>
      <command
            defaultHandler="org.jetbrains.kotlin.ui.commands.ConfigureKotlinActionHandler"
            id="org.jetbrains.kotlin.ui.commands.configuration"
            name="Configure Kotlin">
      </command>
      <command
            defaultHandler="org.jetbrains.kotlin.ui.commands.AddCoroutinesActionHandler"
            id="org.jetbrains.kotlin.ui.commands.configuration.coroutines"
            name="Add Coroutines Library">
      </command>
      <command
            defaultHandler="org.jetbrains.kotlin.ui.commands.findReferences.KotlinFindReferencesInProjectHandler"
            id="org.jetbrains.kotlin.ui.commands.findReferences.findReferencesInProject"
            name="Find References In Project">
      </command>
      <command
            categoryId="org.eclipse.search.ui.category.search"
            defaultHandler="org.jetbrains.kotlin.ui.commands.findReferences.KotlinFindReferencesInWorkspaceHandler"
            description="Find References In Workspace"
            id="org.jetbrains.kotlin.ui.commands.findReferences.findReferencesInWorkspace"
            name="Find References In Workspace">
      </command>
      <command
              defaultHandler="org.jetbrains.kotlin.ui.commands.findReferences.KotlinFindImplementationsInWorkspaceHandler"
              id="org.jetbrains.kotlin.ui.commands.findReferences.KotlinFindImplementationsInWorkspaceHandler"
              name="Find Implementations in Workspace">
      </command>
      <command
              defaultHandler="org.jetbrains.kotlin.ui.commands.findReferences.KotlinFindImplementationsInProjectHandler"
              id="org.jetbrains.kotlin.ui.commands.findReferences.KotlinFindImplementationsInProjectHandler"
              name="Find Implementations in Project">
      </command>
       <command defaultHandler="org.jetbrains.kotlin.ui.editors.navigation.KotlinOpenSuperImplementationActionHandler"
                id="org.jetbrains.kotlin.ui.editors.navigation.KotlinOpenSuperImplementationActionHandler"
                name="Super Implementations"/>
       <command defaultHandler="org.jetbrains.kotlin.ui.editors.navigation.KotlinOpenAllSuperImplementationActionHandler"
                id="org.jetbrains.kotlin.ui.editors.navigation.KotlinOpenAllSuperImplementationActionHandler"
                name="All Super Implementations"/>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.jetbrains.kotlin.ui.editors.KotlinFileEditor.EditorContext">
         <command
               commandId="org.jetbrains.kotlin.psi.visualization"
               label="View Psi Structure of current file"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.jdt.ui.PackageExplorer">
         <command
               commandId="org.jetbrains.kotlin.ui.commands.j2k"
               label="Convert Java to Kotlin"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.jdt.ui.PackageExplorer">
         <menu
               label="Configure Kotlin">
            <command
                  commandId="org.jetbrains.kotlin.ui.commands.deconfiguration"
                  label="Remove Kotlin Nature"
                  style="push">
               <visibleWhen
                     checkEnabled="true">
               </visibleWhen>
            </command>
            <command
                  commandId="org.jetbrains.kotlin.ui.commands.configuration"
                  label="Add Kotlin Nature"
                  style="push">
               <visibleWhen
                     checkEnabled="true">
               </visibleWhen>
            </command>
            <command
                  commandId="org.jetbrains.kotlin.ui.commands.configuration.coroutines"
                  label="Add Coroutines Library"
                  style="push">
               <visibleWhen
                     checkEnabled="true">
               </visibleWhen>
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.jetbrains.kotlin.ui.editors.KotlinFileEditor.EditorContext">
         <menu
               label="References">
            <command
                  commandId="org.jetbrains.kotlin.ui.commands.findReferences.findReferencesInProject"
                  label="References in Project"
                  style="push">
            </command>
            <command
                  commandId="org.jetbrains.kotlin.ui.commands.findReferences.findReferencesInWorkspace"
                  label="References in Workspace"
                  style="push">
            </command>
            <command
                  commandId="org.jetbrains.kotlin.ui.commands.findReferences.KotlinFindImplementationsInWorkspaceHandler"
                  label="Implementations in Workspace"
                  style="push">
            </command>
            <command
                  commandId="org.jetbrains.kotlin.ui.commands.findReferences.KotlinFindImplementationsInProjectHandler"
                  label="Implementations in Project"
                  style="push">
            </command>
             <command commandId="org.jetbrains.kotlin.ui.editors.navigation.KotlinOpenSuperImplementationActionHandler"
                      label="Super Implementations" style="push"/>
             <command
                     commandId="org.jetbrains.kotlin.ui.editors.navigation.KotlinOpenAllSuperImplementationActionHandler"
                     label="All Super Implementations" style="push"/>
         </menu>
      </menuContribution>
   </extension>
   <extension
         id="org.jetbrains.kotlin.ui.marker"
         point="org.eclipse.core.resources.markers">
      <super
            type="org.eclipse.core.resources.marker">
      </super>
      <super
            type="org.eclipse.core.resources.textmarker">
      </super>
      <persistent
            value="true">
      </persistent>
   </extension>
   <extension
         point="org.eclipse.ui.editors.markerAnnotationSpecification">
      <specification
            annotationImageProvider="org.jetbrains.kotlin.ui.editors.annotations.KotlinAnnotationImageProvider"
            annotationType="org.jetbrains.kotlin.ui.annotation.error"
            colorPreferenceKey="org.jetbrains.kotlin.ui.annotation.error.color"
            colorPreferenceValue="255,0,128"
            highlightPreferenceKey="org.jetbrains.kotlin.ui.annotation.error.highlight"
            label="Kotlin Compilation Error"
            overviewRulerPreferenceKey="org.jetbrains.kotlin.ui.annotation.error.overview"
            overviewRulerPreferenceValue="true"
            symbolicIcon="error"
            textPreferenceKey="org.jetbrains.kotlin.ui.annotation.error.text"
            textPreferenceValue="true"
            textStylePreferenceKey="org.jetbrains.kotlin.ui.annotation.error.textstyle"
            textStylePreferenceValue="PROBLEM_UNDERLINE"
            verticalRulerPreferenceKey="org.jetbrains.kotlin.ui.annotation.error.ruler"
            verticalRulerPreferenceValue="true">
      </specification>
      <specification
            annotationImageProvider="org.jetbrains.kotlin.ui.editors.annotations.KotlinAnnotationImageProvider"
            annotationType="org.jetbrains.kotlin.ui.annotation.warning"
            colorPreferenceKey="org.jetbrains.kotlin.ui.annotation.warning.color"
            colorPreferenceValue="244,200,45"
            highlightPreferenceKey="org.jetbrains.kotlin.ui.annotation.warning.highlight"
            label="Kotlin Compilation Warning"
            overviewRulerPreferenceKey="org.jetbrains.kotlin.ui.annotation.warning.overview"
            overviewRulerPreferenceValue="true"
            symbolicIcon="warning"
            textPreferenceKey="org.jetbrains.kotlin.ui.annotation.warning.text"
            textPreferenceValue="true"
            textStylePreferenceKey="org.jetbrains.kotlin.ui.annotation.warning.textstyle"
            textStylePreferenceValue="PROBLEM_UNDERLINE"
            verticalRulerPreferenceKey="org.jetbrains.kotlin.ui.annotation.warning.ruler"
            verticalRulerPreferenceValue="true">
      </specification>
      <specification
            annotationType="org.jetbrains.kotlin.ui.annotation.smartCast"
            colorPreferenceKey="org.jetbrains.kotlin.ui.annotation.smartCast.color"
            colorPreferenceValue="219,255,219"
            highlightPreferenceKey="org.jetbrains.kotlin.ui.annotation.smartCast.highlight"
            highlightPreferenceValue="true"
            label="Kotlin Smart Cast"
            overviewRulerPreferenceKey="org.jetbrains.kotlin.ui.annotation.smartCast.overview"
            overviewRulerPreferenceValue="true"
            textPreferenceKey="org.jetbrains.kotlin.ui.annotation.smartCast.text"
            textPreferenceValue="true"
            textStylePreferenceValue="NONE"
            verticalRulerPreferenceKey="org.jetbrains.kotlin.ui.annotation.smartCast.ruler"
