      </specification>
   </extension>
   <extension
         point="org.eclipse.ui.editors.annotationTypes">
      <type
            markerSeverity="1"
            markerType="org.jetbrains.kotlin.ui.marker"
            name="org.jetbrains.kotlin.ui.annotation.error"
            super="org.eclipse.ui.workbench.texteditor.error">
      </type>
      <type
            markerSeverity="2"
            markerType="org.jetbrains.kotlin.ui.marker"
            name="org.jetbrains.kotlin.ui.annotation.warning"
            super="org.eclipse.ui.workbench.texteditor.warning">
      </type>
      <type
            name="org.jetbrains.kotlin.ui.annotation.smartCast">
      </type>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchShortcuts">
      <shortcut
            class="org.jetbrains.kotlin.ui.launch.KotlinLaunchShortcut"
            icon="icons/kotlin-file.gif"
            id="org.jetbrains.kotlin.ui.launch.launchShortcut"
            label="Kotlin Application"
            modes="run,debug">
         <contextualLaunch>
            <enablement>
               <with
                     variable="selection">
                  <count
                        value="1">
                  </count>
                  <iterate
                        operator="and">
                     <test
                           property="org.jetbrains.kotlin.ui.launching.canLaunchAsApplication">
                     </test>
                  </iterate>
               </with>
            </enablement>
         </contextualLaunch>
      </shortcut>
      <shortcut
            class="org.jetbrains.kotlin.ui.launch.junit.KotlinJUnitLaunchShortcut"
            icon="icons/julaunch.gif"
            id="org.jetbrains.kotlin.ui.launch.junit"
            label="Kotlin JUnit Test"
            modes="run,debug">
         <contextualLaunch>
            <enablement>
               <with
                     variable="selection">
                  <count
                        value="1">
                  </count>
                  <iterate
                        operator="and">
                     <test
                           property="org.jetbrains.kotlin.ui.launching.canLaunchAsJUnit">
                     </test>
                  </iterate>
               </with>
            </enablement>
         </contextualLaunch>
      </shortcut>
      <shortcut
            class="org.jetbrains.kotlin.ui.launch.KotlinScriptLaunchShortcut"
            icon="icons/kotlin-file.gif"
            id="org.jetbrains.kotlin.ui.launch.scriptLaunchShortcut"
            label="Kotlin Script"
            modes="run">
         <contextualLaunch>
            <enablement>
               <with
                     variable="selection">
                  <count
                        value="1">
                  </count>
                  <iterate
                        operator="and">
                     <test
                           property="org.jetbrains.kotlin.ui.launching.canLaunchAsScript">
                     </test>
                  </iterate>
               </with>
            </enablement>
         </contextualLaunch>
      </shortcut>
   </extension>
   <extension
         point="org.eclipse.debug.core.statusHandlers">
      <statusHandler
            class="org.jetbrains.kotlin.ui.launch.CompilerStatusHandler"
            code="1"
            id="org.jetbrains.kotlin.ui.statusHandler"
            plugin="org.jetbrains.kotlin.core">
      </statusHandler>
   </extension>
   <extension
         id="org.jetbrains.kotlin.ui.kotlinBuilder"
         name="KotlinBuilder"
         point="org.eclipse.core.resources.builders">
      <builder callOnEmptyDelta="true">
         <run
               class="org.jetbrains.kotlin.ui.builder.KotlinBuilder">
         </run>
      </builder>
   </extension>
   <extension
         point="org.eclipse.ui.editors.templates">
      <include
            file="templates/default-templates.xml"
            translations="templates/default-templates.xml">
      </include>
      <contextType
            class="org.jetbrains.kotlin.ui.editors.templates.KotlinTemplateContextType"
            id="kotlin-top-level-declarations"
            name="Kotlin Top Level Declarations"
            registryId="org.jetbrains.kotlin.ui.editors.KotlinFileEditor">
      </contextType>
      <contextTypeRegistry
            id="org.jetbrains.kotlin.ui.editors.KotlinFileEditor">
      </contextTypeRegistry>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
